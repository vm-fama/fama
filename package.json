{
  "name": "fama",
  "version": "1.0.0",
  "description": "FAMA",
  "main": "index.js",
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb/hooks"
    ],
    "env": {
      "browser": true,
      "es6": true
    }
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon index.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vm-fama/fama.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vm-fama/fama/issues"
  },
  "homepage": "https://github.com/vm-fama/fama#readme",
  "dependencies": {
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^7.15.1",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0"
  }
